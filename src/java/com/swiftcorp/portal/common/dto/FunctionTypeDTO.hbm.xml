<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
 <hibernate-mapping>
  <class name="com.swiftcorp.portal.common.dto.FunctionTypeDTO" table="functiontype">
	  <id name="componentId" column="componentId" type="long">
	    <generator class="increment" /> 
	  </id>
	  <property name="uniqueCode" type="string" column="uniqueCode" length="50"  /> 
	  <property name="typeName" type="string" column="typeName" length="50"  /> 
	  <property name="typeId" type="int" column="typeId" length="50"  />   
	   </class>
   
 </hibernate-mapping>

<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
 <hibernate-mapping>
  <class name="com.swiftcorp.portal.item.dto.ItemDTO" table="item">
	  <id name="componentId" column="componentId" type="long">
	    <generator class="increment" /> 
	  </id>
	  <!--
	  <property name="uniqueCode" type="string" column="uniquecode" length="50"  />
	  --> 
	<property name="itemId" type="string" column="itemid" /> 
	<property name="itemName" type="string" column="itemname" /> 
	<property name="parent" type="long" column="parent" />
	<property name="purchasePrice" type="double" column="purchaseprice" /> 
	<property name="sellPrice" type="double" column="sellprice" />  
	 <!--
	 <property name="description" type="string" column="description"  /> 
	 <property name="status" type="int" column="status"  /> 
     <property name="updatedDate" type="calendar" column="updateddate"  />	
	 <property name="updatedBy" type="long" column="updatedby"  />
	 -->
  </class>
   
 </hibernate-mapping>
